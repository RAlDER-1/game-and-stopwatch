 # # #\n            `)\n        // Reset values\n        playerX = 2\n        delay_before_movement = 500\n        obstacleY = []\n        obstacles = []\n        start_game = 0\n        score = 0\n        waveHits = 0\n        // Delete old sprites\n        if (Player) {\n            Player.delete()\n        }\n        for (let o of obstacles) {\n            o.delete()\n        }\n        // Reset player\n        Player = game.createSprite(playerX, 4)\n        // Create 4 obstacles\n        for (let index = 0; index < 4; index++) {\n            obstacles.push(game.createSprite(Math.randomRange(0, 4), 0))\n            obstacleY.push(0)\n        }\n        switchToMode0 = 0\n        start_game = 1\n    }\n})\n// -----------------------------\n// OBSTACLE MOVEMENT + SCORING + COLLISION\n// -----------------------------\nbasic.forever(function () {\n    if (start_game == 1 && mode == 0) {\n        for (let j = 0; j <= obstacles.length - 1; j++) {\n            obstacleY[j] += 1\n// Reset when reaching bottom\n            if (obstacleY[j] > 4) {\n                // SPEED UP\n                if (delay_before_movement > 250) {\n                    delay_before_movement += 0 - 10\n                } else {\n                    delay_before_movement = 200\n                }\n                // Count obstacles for scoring\n                waveHits += 1\n                if (waveHits == obstacles.length) {\n                    score += 5\n                    waveHits = 0\n                    basic.clearScreen()\n                }\n                obstacleY[j] = 0\n                obstacles[j].set(LedSpriteProperty.X, Math.randomRange(0, 4))\n            }\n            obstacles[j].set(LedSpriteProperty.Y, obstacleY[j])\n            obstacles[j].set(LedSpriteProperty.Brightness, 255)\n            // COLLISION DETECTION\n            if (obstacles[j].get(LedSpriteProperty.X) == playerX && obstacles[j].get(LedSpriteProperty.Y) == 4) {\n                // Show final score\n                basic.clearScreen()\n                // GAME OVER\n                mode = 1\n                start_game = 0\n                basic.showLeds(`\n                    . # . # .\n                    # # # # #\n                    # # # # #\n                    . # # # .\n                    . . # . .\n                    `)\n                // Wait for logo press to restart\n                basic.showString(\"score:\" + score)\n                mode = 0\n                mode = 1\n                switchToMode0 = 0\n                // Show final score\n                basic.clearScreen()\n                switchToMode0 = 1\n            }\n        }\n        basic.pause(delay_before_movement)\n    }\n})\n","README.md":"","pxt.json":"{\n    \"name\": \"game scoring test\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\"\n    ],\n    \"preferredEditor\": \"tsprj\"\n}\n"}},{"timestamp":1764429091567,"editorVersion":"8.0.18","text":{"main.blocks":"<xml xmlns=\"https://developers.google.com/blockly/xml\"><variables><variable id=\"W*+m*YVBKTsp(vobSupP\">menuOption</variable><variable id=\"S@jQ[fZQ/jV{:r)s:FM9\">menuConfirmed</variable><variable id=\"p8~=C{0j:~+Xvz7F:m~y\">mode</variable><variable id=\"|I?q22GwNww7^=+iIvus\">playerX</variable><variable id=\"T2#A}^.=nmYw]0}[_ApR\">Player</variable><variable id=\"#}FR5B%rTc7M0:JxzY:6\">running</variable><variable id=\"#+8d2P}k2;|+wj(^7-:h\">elapsed</variable><variable id=\"4KbiMG}/$9qCA!]sJ,4?\">start</variable><variable id=\"nd4}o0b70;N%73G__;bi\">Last_time</variable><variable id=\"{YgU`;egn(2z?{q~QmF0\">switchToMode0</variable><variable id=\"%R(](6}?1;c~Vc#3r{YG\">seconds</variable><variable id=\"f:;Ng#J.Vq`jo;.Az}Yh\">delay_before_movement</variable><variable id=\"7%^Y4cpoKj)G@JvRMv:J\">obstacleY</variable><variable id=\"MfPXw_tUPS`b;ju,D[vk\"